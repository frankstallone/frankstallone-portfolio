---
export interface Props {
    variant?: Variant;
    title?: string;
}
import { Variant, VARIANT_MAPS } from '../utils/variants.ts'
const { title = 'The portfolio of', desc = 'The UX/UI, logo, branding and front-end development portfolio of Frank Stallone', variant = "default" } = Astro.props;
---

<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, shrink-to-fit=no">
<meta name="description" content={desc}>
<meta name="theme-color" content={`${VARIANT_MAPS[variant].themeColor}`}>
<title>{title}: Frank Stallone</title>
<link rel="preload" as="font" href="/fonts/cartridge/Cartridge-Regular.woff2" type="font/woff2" crossorigin="anonymous">
<link rel="preload" as="font" href="/fonts/cartridge/Cartridge-Regular.woff" type="font/woff" crossorigin="anonymous">
<link rel="preload" as="font" href="/fonts/facundo/Facundo-Black.woff" type="font/woff" crossorigin="anonymous">
