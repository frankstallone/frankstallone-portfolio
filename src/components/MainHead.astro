<link rel="preload" as="font" href="/fonts/facundo/Facundo-Bold.woff" type="font/woff" crossorigin="anonymous">
<link rel="preload" as="font" href="/fonts/facundo/Facundo-BoldIt.woff" type="font/woff" crossorigin="anonymous">
<link rel="preload" as="font" href="/fonts/facundo/Facundo-ExtraLight.woff" type="font/woff" crossorigin="anonymous">
<link rel="preload" as="font" href="/fonts/facundo/Facundo-ExtraLightIt.woff" type="font/woff" crossorigin="anonymous">
<link rel="preload" as="font" href="/fonts/facundo/Facundo-Light.woff" type="font/woff" crossorigin="anonymous">
