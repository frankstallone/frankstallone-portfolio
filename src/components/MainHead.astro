---
const { title = 'The portfolio of', desc = 'The UX/UI, logo, branding and front-end development portfolio of Frank Stallone', variant = "default" } = Astro.props;
import { Variant, VARIANT_MAPS } from '../utils/variants.ts'
export interface Props {
    variant?: Variant;
}
---

<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, shrink-to-fit=no">
<meta name="description" content={desc}>
<meta name="theme-color" content={`${VARIANT_MAPS[variant].themeColor}`}>
<title>{title}: Frank Stallone</title>
<link rel="stylesheet" type="text/css" href="/global.css">
<link rel="stylesheet" type="text/css" href="/facundo.css">
<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
<link rel="manifest" href="/site.webmanifest">
<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#4e2009">
<meta name="msapplication-TileColor" content="#ffffff">